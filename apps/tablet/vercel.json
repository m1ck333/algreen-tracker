{
  "buildCommand": "cd ../.. && pnpm install && pnpm --filter tablet build",
  "outputDirectory": "dist",
  "installCommand": "echo 'skip'",
  "framework": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "Service-Worker-Allowed", "value": "/" }
      ]
    }
  ]
}
